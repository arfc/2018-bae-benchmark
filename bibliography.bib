	keywords = {agent based modeling, Nuclear fuel cycle, Object orientation, simulation, Systems analysis, Computer Science - Computational Engineering, Finance, and Science, Computer Science - Mathematical Software, Computer Science - Multiagent Systems, Computer Science - Software Engineering, D.2.13, D.2.4, I.6.7, I.6.8, nuclear engineering, Finance, and Science, Computer Science - Computational Engineering},
