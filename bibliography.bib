	pages = {1460--1469},
	file = {ScienceDirect Full Text PDF:/home/teddy/Zotero/storage/TB4H4GQ3/Kostevšek et al. - 2016 - The concept of an ecosystem model to support the t.pdf:application/pdf;ScienceDirect Snapshot:/home/teddy/Zotero/storage/7QUMGADH/S0306261916304603.html:text/html;ScienceDirect Snapshot:/home/teddy/Zotero/storage/Z7HZVIMF/S0306261916304603.html:text/html}
